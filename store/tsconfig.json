{
    "compilerOptions": {
        "module": "AMD",
        "noImplicitAny": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "outDir": "static/build",
        "outFile": "static/build/agns.amd.js",
        "sourceMap": true,
        "incremental": true,
        "moduleResolution": "node",
        "target": "ES6"
    },
    "include": [
        "**/*"
    ],
    "exclude": [
        "vuecart",
        "vuex"
    ]
}
